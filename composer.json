{
    "name": "creativecurtis/laramyob",
    "description": "A Laravel wrapper for MYOB Account Right v2",
    "keywords": [
        "creativecurtis",
        "laramyob"
    ],
    "homepage": "https://github.com/creativecurtis/laramyob",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luke Curtis",
            "email": "lukesimoncurtis@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "nesbot/carbon": "^1.26.3",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Mandasa\\Drillcutmyob\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mandasa\\Drillcutmyob\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mandasa\\Drillcutmyob\\DrillcutmyobServiceProvider"
            ],
            "aliases": {
                "Drillcutmyob": "Mandasa\\Drillcutmyob\\DrillcutmyobFacade"
            }
        }
    }
}
